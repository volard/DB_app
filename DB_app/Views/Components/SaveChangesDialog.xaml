<ContentDialog
    CloseButtonClick="ContentDialog_CloseButtonClick"
    CloseButtonText="Cancel"
    Content="You have unsaved changes that will be lost. Would you like to save your changes?"
    PrimaryButtonClick="ContentDialog_PrimaryButtonClick"
    PrimaryButtonText="Save"
    SecondaryButtonClick="ContentDialog_SecondaryButtonClick"
    SecondaryButtonText="Don't save"
    Title="Save changes?"
    mc:Ignorable="d"
    x:Class="DB_app.Views.SaveChangesDialog"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="using:DB_app.Views"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <ContentDialog.Resources>
        <Style BasedOn="{StaticResource DefaultContentDialogStyle}" TargetType="local:SaveChangesDialog" />
    </ContentDialog.Resources>
</ContentDialog>
